{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run Deemind Build",
      "type": "process",
      "command": "node",
      "args": ["cli.js", "demo", "--auto"],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": []
    },
    {
      "label": "Run Deemind Validate",
      "type": "shell",
      "command": "npm run deemind:validate",
      "options": {
        "shell": {
          "executable": "bash",
          "args": ["-lc"]
        }
      },
      "problemMatcher": []
    },
    {
      "label": "Watch New Themes",
      "type": "shell",
      "command": "npm run deemind:watch",
      "options": {
        "shell": {
          "executable": "bash",
          "args": ["-lc"]
        }
      },
      "problemMatcher": []
    }
  ]
}
